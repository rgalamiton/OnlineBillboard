
@model OnlineBillboard.Models.BillboardDetails

@{ 
    ViewData["Title"] = "Details";
}

<h1> Billboard Details</h1>

<table class="table">
    <thead>
        <tr>
            <th> Title</th>
            <th> Location</th>
            <th> Events</th>
            <th> Description</th>
        </tr>
    </thead>
    <tbody>
       
        <tr>
            <td>
                @Model.title
            </td>
            <td>
                @Model.location
            </td>
            <td>
                @Model.events
            </td>
            <td>
                @Model.description
            </td>
            
        </tr>
        
    </tbody>
</table>

<br />
<a asp-action="Index" asp-controller="Billboard"> Go back to List </a>